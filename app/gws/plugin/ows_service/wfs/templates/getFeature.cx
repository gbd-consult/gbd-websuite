@include ../../templatelib.cx

@def formatter(feature_caps)
    @tag {feature_caps.xname.q} gml:id="{feature_caps.feature.uid}"
        @each feature_caps.feature.attributes as a
            @t {feature_caps.xname.ns.name}:{a.name} {a.value}
        @end
        @tag {feature_caps.xname.ns.name}:geometry
            @insert {feature_caps.shape_tag}
        @end
    @end
@end

@wfs_feature_collection(ARGS.collection, formatter)

