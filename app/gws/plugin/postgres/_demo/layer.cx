projects+ {
    title "Postgres layer"
    metadata.abstract "`postgres` layers display vector features from Postgis tables."

    map {
        center {demo_point_duesseldorf_3857}
        zoom.initScale 30000
        zoom.maxScale  1000000
    }

    map.layers+ {
        title "POIs"
        type "postgres"
        tableName "edit.poi"
    }

    map.layers+ {
        title "Districts"
        type "postgres"
        tableName "edit.district"
    }

    {demo_osm_background}
}
