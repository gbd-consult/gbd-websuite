projects+ {
    uid "gws.plugin.tile_layer.demos.client"
    title "Tile Layer: Client Display"

    templates+ {
        subject "project.description"
        type "html"
        text """
            OSM layer, displayed directly on the client.
        """
    }

    @demo_map(
        crs=3857
        centerAround=[DEMO_DUESSELDORF_3857, 500]
        zoomScales=DEMO_OSM_ZOOM_SCALES
        initScale=150000
    )

    map.layers+ {
        @demo_osm_layer
        display "client"
    }
}
