projects+ {
    title "Tile layer, server-side"
    metadata.abstract "Tile layer, cached on the server."

    map {
        crs 3857
        center {demo_point_duesseldorf_3857}
        zoom.initScale 150000
    }

    map.layers+ {
        title "Open Street Map"
        type "tile"
        provider.url "https://osmtiles.gbd-consult.de/ows/{{z}}/{{x}}/{{y}}.png"

        display "tile"
        options.cache true
    }
}
