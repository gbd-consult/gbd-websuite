{
    "compilerOptions": {
        "baseUrl": ".",
        "incremental": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "jsx": "react",
        "lib": [
            "es2019",
            "dom"
        ],
        "module": "CommonJS",
        "noEmitOnError": true,
        "noImplicitAny": false,
        "outDir": "../__build/js",
        "paths": {
            "@build/*": [ "../__build/*" ],
            "gws.base/*": [ "../gws/base/*" ],
            "*": ["./node_modules/*", "./src/*"]
        },
        "preserveWatchOutput": true,
        "rootDir": "/",
        "target": "es2019"
    }
}
