
auth.methods+ { 
    type "web" 
    secure true
    loginRedirect {
        pattern "^/(demo|project)"
        target "/login"
    }
}

auth.methods+ { type "basic" secure true }

auth.providers+ { type "file" path "/demos/users.json" }

auth.session { type "sqlite" }

actions+ {
    type "auth"
    permissions.read "allow all"
}
