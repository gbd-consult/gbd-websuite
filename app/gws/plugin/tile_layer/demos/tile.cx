projects+ {
    title "Tile Layer: Server Display"
    metadata.abstract "OSM layer, cached on the server."

    map {
        crs 3857
        center {DEMO_DUESSELDORF_3857}
        zoom.initScale 150000
    }

    map.layers+ {
        title "Open Street Map"
        type "tile"
        url "https://osmtiles.gbd-consult.de/ows/{{z}}/{{x}}/{{y}}.png"

        display "tile"
        cache.enabled true
    }
}
